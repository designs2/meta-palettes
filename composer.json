{
    "name": "designs2/meta-palettes",
    "description": "MetaPalettes extension for Contao OpenSource CMS",
    "keywords": [
        "contao",
        "page",
        "title"
    ],
    "type": "contao-module",
    "homepage": "http://bit3.de",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Christian Schiffler",
            "email": "c.schiffler@cyberspectrum.de",
            "homepage": "http://www.cyberspectrum.de",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "info@bit3.de",
        "issues": "https://github.com/bit3/contao-meta-palettes/issues",
        "wiki": "https://github.com/bit3/contao-meta-palettes/wiki",
        "irc": "irc://irc.freenode.org/contao.dev",
        "source": "https://github.com/designs2/meta-palettes"
    },
    "require": {
        "php": ">=5.3",
        "contao/core": ">=2.11,<4",
        "contao-community-alliance/composer-plugin": "*"
    },
    "require-dev": {
        "bit3/php-coding-standard": "*",
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "*",
        "phpmd/phpmd": "*",
        "contao-community-alliance/dc-general": "*"
    },
    "replace": {
        "contao-legacy/metapalettes": "self.version",
        "bit3/contao-meta-palettes": "self.version"
    },
    "conflict": {
        "metamodels/dc_general": "<1-dev"
    },
    "autoload": {
        "psr-4": {
            "Bit3\\Contao\\MetaPalettes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bit3\\Contao\\MetaPalettes\\Test\\": "tests/"
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "contao": "system/modules/metapalettes"
            }
        },
        "branch-alias": {
            "dev-master": "1.9.x-dev",
            "dev-develop": "1.10.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
